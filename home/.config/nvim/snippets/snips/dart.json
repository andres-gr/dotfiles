{
	// Place your snippets for dart here. Each snippet is defined under a snippet name and has a prefix, body and 
	// description. The prefix is what is used to trigger the snippet and the body will be expanded and inserted. Possible variables are:
	// $1, $2 for tab stops, $0 for the final cursor position, and ${1:label}, ${2:another} for placeholders. Placeholders with the 
	// same ids are connected.
	// Example:
	// "Print to console": {
	// 	"prefix": "log",
	// 	"body": [
	// 		"console.log('$1');",
	// 		"$2"
	// 	],
	// 	"description": "Log output to console"
	// }
    "Annonymous function": {
        "prefix": "afunc",
        "body": "(${1:params}) {$2}$0",
        "description": "Inline annonymous function"
    },
    "Return named arrow function": {
        "prefix": "narfunc",
        "body": "${1:void} ${2:name}(${3:params}) => $0",
        "description": "Return named arrow function"
    },
    "Return annonymous arrow function": {
        "prefix": "arfunc",
        "body": "(${2:params}) => $0",
        "description": "Return annonymous arrow function"
    },
    "Single line function": {
        "prefix": "sfunc",
        "body": "${1:void} ${2:name}(${3:params}) { $4; }$0",
        "description": "Single line named function"
    },
    "Ternary if": {
        "prefix": "ift",
        "body": "${1:condition} ? ${2:true} : ${3:false}$0",
        "description": "Ternary if expression"
    }
}
